# transparency - true or false (default)
URxvt*transparent: true

# tint with any color; i.e., blue, red, tomato4, olivedrab2, etc.
URxvt*tintColor: Gray

# scrollback buffer lines - 65535 is max on most machines (64 is default)
URxvt*saveLines: 12000

# font color (default is black)
URxvt*foreground: White

# background color (prior to tinting) (default is white)
URxvt*background: Gray

# font
URxvt*font: xft:Inconsolata:pixelsize=14:antialias=true:hinting=true
URxvt*boldFont: xft:Inconsolata:bold:pixelsize=14:antialias=true:hinting=true

#--------------------#
# Custom colorscheme #
#--------------------#

# Black / Dark Grey

# Dark / Light Red
*color1: #CC9393
*color9: #CC9393

# Dark / Light Green
*color2: #CAE682
*color10: #CAE682

# Dark / Light Yellow

# Dark / Light Blue
*color4: #8DCBE2
*color12: #8DCBE2

# Dark / Light Magenta
*color5: #EAB882
*color13: #EAB992

# Dark / Light Cyan
*color6: #9393CC
*color14: #9393CC

# Grey / White
*color7: #A0A0A0
*color15: E2E2E2

# scrollbar - true (default) or false
URxvt*scrollBar: false

# scrollbar position - left=false (default) or right=true
URxvt*scrollBar_right: false

# scrollbar style - rxvt (default), plain, next, or xterm
URxvt*scrollstyle: rxvt
