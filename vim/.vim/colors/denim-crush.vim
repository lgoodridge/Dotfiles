hi Title	guifg=#E07E2D
hi Visual   gui=none guibg=#7391C7 guifg=#E3E0D3
hi WarningMsg	guifg=#E07E2D
hi MatchParen guibg=grey30 guifg=#dfffdf

" syntax highlighting groups
hi Comment	guifg=#94948F
hi Constant	guifg=#E07E2D
hi Identifier	guifg=#E0965A
hi Statement	guifg=#E0965A
hi PreProc	guifg=#E07E2D
